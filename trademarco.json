{
  "Name": "PocketOptionAutoTrade",
  "CreationDate": "2025-07-24",
  "Commands": [
    {
      "Command": "store",
      "Target": "${!json}",
      "Value": "tradeData"
    },
    {
      "Command": "echo",
      "Target": "Trading Pair: ${tradeData.pair}",
      "Value": ""
    },
    {
      "Command": "echo",
      "Target": "Expiry: ${tradeData.expiry} min",
      "Value": ""
    },
    {
      "Command": "echo",
      "Target": "Amount: ${tradeData.amount}",
      "Value": ""
    },
    {
      "Command": "click",
      "Target": "css=select#pairSelector",
      "Value": ""
    },
    {
      "Command": "select",
      "Target": "css=select#pairSelector",
      "Value": "${tradeData.pair}"
    },
    {
      "Command": "type",
      "Target": "css=input#amountInput",
      "Value": "${tradeData.amount}"
    },
    {
      "Command": "select",
      "Target": "css=select#expirySelect",
      "Value": "${tradeData.expiry}"
    },
    {
      "Command": "click",
      "Target": "css=button#tradeButton",
      "Value": ""
    },
    {
      "Command": "echo",
      "Target": "Trade placed successfully",
      "Value": ""
    }
  ]
}
